{
  "expo": {
    "name": "IATOK",
    "slug": "iatok",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "iatok",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iatok.app"
    },
    "android": {
      "package": "com.iatok.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}